{
  "name": "@cabalse/ttmfe-component-lib",
  "version": "13.5.0",
  "type": "module",
  "main": "dist/ttmfe-component-lib.cjs.js",
  "module": "dist/ttmfe-component-lib.esm.js",
  "exports": {
    ".": {
      "import": "./dist/ttmfe-component-lib.es.js",
      "require": "./dist/ttmfe-component-lib.umd.cjs"
    },
    "./styles.css": {
      "import": "./dist/ttmfe-component-lib-style.css",
      "require": "./dist/ttmfe-component-lib-style.css"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:css",
    "build:css": "tailwindcss -m -i ./src/components/components.css -o ./dist/ttmfe-component-lib-style.css",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "peerDependencies": {
    "@tailwindcss/vite": "^4.1.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^26.1.0",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.1",
    "@tailwindcss/cli": "^4.1.4"
  }
}
